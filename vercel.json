{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "My_CV/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    },
    {
      "src": "/Images/(.*)",
      "dest": "/My_CV/Images/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/My_CV/fonts/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/My_CV/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/My_CV/index.html"
    }
  ]
}